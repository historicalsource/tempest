	.MACRO INVAL		;MACRO TO CALCULATE ENTRIES FOR
				;INVERSE AND EXPONENT TABLES
				;INPUT:.BOT=# TO INVERT (0-FF)
				;OUTPUT:.EXP=EXPONENT(POWER OF 2)
				;	.INV=MANTISSA(& SIGN BITS)
				;
	.IF EQ,.BOT
	.INV=0
	.EXP=0
	.MEXIT			;1/0 IS UNDERFINED SO ABORT
	.ENDC
	.EXP=-7
	.INV=7FFF/.BOT
	.REPT 8
	.IF EQ,.INV&0FF00
	.MEXIT
	.ENDC
	.EXP=.EXP+1
	.INV=.INV/2
	.ENDR
	.ENDM			;NEVER GET HERE
				;BUILD INVERSE(MANTISSA PORTION)TABLE
	.MACRO INVBLD
	.BOT=0
	.REPT 100		;LOOP FOR 0 -> 0FF
	INVAL
	.BYTE .INV
	.BOT=.BOT+1
	.ENDR
	.ENDM
	.MACRO EXPBLD		;BUILD INVERSE (EXPONENT PORTION)TABLE
	.BOT=0
	.REPT 100		;LOOP FOR 0 -> 0FF
	INVAL
	.BYTE .EXP
	.BOT=.BOT+1
	.ENDR
	.ENDM
	
