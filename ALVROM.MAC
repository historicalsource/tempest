	CNTR
	SCAL 1,0
	VCTR MX,0,0		;HORIZONTAL LINE ACCROSS MIDDLE
	VCTR -2*MX,0,6
	VCTR 40,1C0,0		;POSITION FOR REPORT OF BAD ROM
	RTSL			;FALL INTO SCREEN BOUNDARIES
MX	=500.
MY	=540.
BONDRY:	CSTAT WHITE		;SCREEN BOUNDARY
	SCAL 1,0
VORBOX::	CNTR
	VCTR -MX,-MY,0
	VCTR 2*MX,0,6
	VCTR 0,2*MY,6
	VCTR -2*MX,0,6
	VCTR 0,-2*MY,6
